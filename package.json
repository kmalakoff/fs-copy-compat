{
  "name": "fs-copy-compat",
  "version": "1.0.0",
  "description": "Cross-platform file copy utilities with Node.js 0.8+ compatibility. Provides copyFile, copyFileSync, and cpSync with optimized fallbacks for older Node versions.",
  "keywords": [
    "fs",
    "copy",
    "copyFile",
    "copyFileSync",
    "cpSync",
    "polyfill",
    "ponyfill",
    "compat",
    "node-0.8"
  ],
  "homepage": "https://github.com/kmalakoff/fs-copy-compat",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kmalakoff/fs-copy-compat.git"
  },
  "license": "MIT",
  "author": "Kevin Malakoff <kmalakoff@gmail.com> (https://github.com/kmalakoff)",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/cjs/index.js",
  "source": "src/index.ts",
  "types": "dist/cjs/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsds build",
    "format": "tsds format",
    "prepublishOnly": "tsds validate",
    "test": "tsds test:node --no-timeouts",
    "test:engines": "nvu engines tsds test:node --no-timeouts",
    "version": "tsds version"
  },
  "dependencies": {
    "is-absolute": "^1.0.0",
    "mkdirp-classic": "^0.5.2",
    "pump": "^3.0.3"
  },
  "devDependencies": {
    "@types/mocha": "*",
    "@types/node": "*",
    "fs-remove-compat": "^1.0.0",
    "node-version-use": "*",
    "ts-dev-stack": "*",
    "tsds-config": "*"
  },
  "engines": {
    "node": ">=0.8"
  }
}
